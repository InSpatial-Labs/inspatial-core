{
  "name": "@inspatial/util",
  "version": "0.0.1",
  "private": true,
  "tasks": {
    "dev": "deno run --watch --allow-read --allow-write --allow-net --allow-env src/index.ts",
    "test": "deno test --coverage",
    "clean": "rm -rf node_modules && rm -rf deno.lock"
  },
  "imports": {
    "date-fns": "npm:date-fns@^4.1.0",
    "react-day-picker": "npm:react-day-picker@^9.1.3",
    "clsx": "npm:clsx@^2.1.1",
    "tailwind-merge": "npm:tailwind-merge@^1.14.0"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "publish": {
    "exclude": [
      ".github",
      ".vscode",
      "examples",
      "fixtures",
      "**/*.test.ts",
      "test_deps.ts"
    ]
  }
}
