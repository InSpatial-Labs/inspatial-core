{
  "name": "@inspatial/kit",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "Apache-2.0",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./button": {
      "types": "./dist/button/index.d.ts",
      "import": "./dist/button/index.mjs",
      "require": "./dist/button/index.js"
    },
    "./structure": {
      "types": "./dist/structure/index.d.ts",
      "import": "./dist/structure/index.mjs",
      "require": "./dist/structure/index.js"
    },
    "./ornament": {
      "types": "./dist/ornament/index.d.ts",
      "import": "./dist/ornament/index.mjs",
      "require": "./dist/ornament/index.js"
    },
    "./presentation": {
      "types": "./dist/presentation/index.d.ts",
      "import": "./dist/presentation/index.mjs",
      "require": "./dist/presentation/index.js"
    }
  },
  "files": [
    "dist/**",
    "button",
    "structure",
    "ornament",
    "presentation"
  ],
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "pub:preview": "pnpm publish --no-git-checks --access public --tag preview",
    "pub:canary": "pnpm publish --no-git-checks --access public --tag canary",
    "pub:stable": "pnpm publish --access public"
  },
  "devDependencies": {
    "@inspatial/eslint-config": "workspace:*",
    "@inspatial/tsconfig": "workspace:*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
