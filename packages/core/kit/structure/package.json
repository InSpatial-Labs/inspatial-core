{
  "name": "@inspatial/kit-structure",
  "description": "InSpatial kit Structure",
  "version": "0.0.1",
  "sideEffects": false,
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "src/**",
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "pub:preview": "pnpm publish --no-git-checks --access public --tag preview",
    "pub:canary": "pnpm publish --no-git-checks --access public --tag canary",
    "pub:stable": "pnpm publish --access public"
  },
  "devDependencies": {
    "@inspatial/eslint-config": "workspace:*",
    "@inspatial/tsconfig": "workspace:*",
    "@types/react": "^18.3.11",
    "eslint": "^8.57.0",
    "glob": "^11.0.0",
    "react": "^18.3.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "2.1.1"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "react-day-picker": "^9.1.3",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.5.3"
  }
}
